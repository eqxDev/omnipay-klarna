{
  "name": "alegra/omnipay-klarna",
  "type": "library",
  "description": "Klarna gateway for Omnipay payment processing library",
  "homepage": "https://github.com/alegraio/omnipay-klarna",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Omnipay\\Klarna\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OmnipayTest\\Klarna\\": "tests/"
    }
  },
  "require": {
    "php": "^7.3|^8",
    "omnipay/common": "^3",
    "ext-json": "*",
    "ext-simplexml": "*",
    "ext-dom": "*",
    "ext-soap": "*"
  },
  "require-dev": {
    "omnipay/tests": "^3",
    "squizlabs/php_codesniffer": "^3"
  },
  "prefer-stable": true
}
